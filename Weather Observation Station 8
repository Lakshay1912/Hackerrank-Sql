select distinct(CITY) from STATION
where CITY like 'A%a'
or CITY like 'A%e'
or CITY like 'A%i'
or CITY like 'A%u'
or CITY like 'A%o'
or CITY like 'E%e'
or CITY like 'E%a'
or CITY like 'E%i'
or CITY like 'E%o'
or CITY like 'E%u'
or CITY like 'O%o'
or CITY like 'O%i'
or CITY like 'O%e'
or CITY like 'O%a'
or CITY like 'O%u'
or CITY like 'U%u'
or CITY like 'U%a'
or CITY like 'U%e'
or CITY like 'U%i'
or CITY like 'U%o'
or CITY like 'I%i'
or CITY like 'I%a'
or CITY like 'I%e'
or CITY like 'I%o'
or CITY like 'I%u';
